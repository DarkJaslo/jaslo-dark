			"name": "Class reference",
			"scope":[
				"entity.name.scope-resolution.function.definition.cpp"
			],
			"settings": {
				"foreground": "#6d660f",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function",
			"scope":[
				"entity.name.function.definition.cpp"
			],
			"settings": {
				"foreground": "#c0b516",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Class word",
			"scope":[
				"storage.type.class.cpp",
				"storage.type.namespace.directive.cpp"
			],
			"settings": {
				"foreground": "#c0b516",
				"fontStyle": "italic bold"
			}
