{
	"name": "Jaslo Dark",
	"colors": {
		"editor.background": "#242424",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#728445"
			}
		},
		{
			"name": "Control Keyword",
			"scope":[
				"keyword.control"
			],
			"settings": {
				"foreground": "#d10f93",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator Keyword",
			"scope":[
				"keyword.operator",
				"punctuation.separator.delimiter.comma.cpp"
			],
			"settings": {
				"foreground": "#9960b6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator Keyword",
			"scope":[
				"keyword.operator.arithmetic.cpp"
			],
			"settings": {
				"foreground": "#741545",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variable",
			"scope":[
				"variable",
				"variable.other.property"
			],
			"settings": {
				"foreground": "#98c4f2",
				"fontStyle": ""
			}
		},
		{
			"name": "String",
			"scope":[
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#ffee00",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "True&False",
			"scope":[
				"constant.language.false",
				"constant.language.true"
			],
			"settings": {
				"foreground": "#ffee00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Semicolon",
			"scope":[
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#3a22d5",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Basic keyword",
			"scope":[
				"storage.type.built-in.primitive.cpp",
				"storage.modifier.specifier.const.cpp",
