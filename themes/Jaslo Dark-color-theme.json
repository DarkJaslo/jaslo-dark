			],
			"settings": {
				"foreground": "#faf6c3",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Number",
			"scope":[
				"constant.numeric.decimal.cpp"
			],
			"settings": {
				"foreground": "#1abf6a",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Macro",
			"scope":[
				"entity.name.function.preprocessor.cpp"
			],
			"settings": {
				"foreground": "#0782cf",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Template",
			"scope":[
				"storage.type.template.cpp",
